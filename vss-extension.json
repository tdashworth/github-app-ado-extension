{
  "$schema": "https://raw.githubusercontent.com/SchemaStore/schemastore/master/src/schemas/json/vss-extension.json",
  "manifestVersion": 1,
  "id": "GitHubAppClient",
  "name": "GitHub App Client",
  "version": "1.0.3",
  "publisher": "tdashworth",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "A set of tasks that call the GitHub API as a provided GitHub App (bot).",
  "categories": [
    "Azure Pipelines"
  ],
  "icons": {
    "default": "icon.png"
  },
  "content": {
    "details": {
      "path": "README.md"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/tdashworth/github-app-ado-extension"
  },
  "public": true,
  "files": [
    {
      "path": "tasks/GitHubAppCreateIssueComment"
    },
    {
      "path": "tasks/GitHubAppCreateRelease"
    },
    {
      "path": "tasks/GitHubAppDeleteIssueComment"
    },
    {
      "path": "tasks/GitHubAppRestRequest"
    }
  ],
  "contributions": [
    {
      "id": "GitHubAppCreateIssueComment",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "tasks/GitHubAppCreateIssueComment"
      }
    },
    {
      "id": "GitHubAppCreateRelease",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "tasks/GitHubAppCreateRelease"
      }
    },
    {
      "id": "GitHubAppDeleteIssueComment",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "tasks/GitHubAppDeleteIssueComment"
      }
    },
    {
      "id": "GitHubAppRestRequest",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "tasks/GitHubAppRestRequest"
      }
    }
  ]
}